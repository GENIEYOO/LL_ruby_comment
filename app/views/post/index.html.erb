<div class="container">
        <div clss = "row">
        <h1>게시글 목록</h1><a href = "/post/new"><h1>게시글작성</h1></a>
        </div>
</div>



<div class = "container">
    <div class = "row">
        
        
    </div>
                    <table class="table">
                      <thead>
                        <tr>
                          <th scope="col">번호</th>
                          <th scope="col">제목</th>
                          <th scope="col">글쓴이</th>
                          <th scope="col">작성일</th>
                        </tr>
                      </thead>
                      <tbody>
                          
                          <% @posts.each do |post| %>
                          <tr>
                              <th scope="row"><%=post.id %></th>
                              <td><%=post.title %></td>
                              <td><%=post.user.email %></td>
                              <td> <%= time_ago_in_words(post.created_at) %></td>
                    <!--post show로 보내줘야 하는데 글아이디와 함꼐-->
                        </tr>
                            <% end %>
                    
                      </tbody>
                    </table>
      <div class = "row">
      </div>     
                    
                    
